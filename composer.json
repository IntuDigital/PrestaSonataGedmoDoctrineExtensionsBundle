{
    "name": "presta/sonata-gedmo-doctrine-extensions-bundle",
    "type": "symfony-bundle",
    "description": "PrestaSonataGedmoDoctrineExtensions : integrates gedmo doctrine extenions in Sonata Admin",
    "keywords": ["symfony2", "bundle", "sonata-admin", "prestaconcept", "gedmo"],
    "homepage": "https://github.com/prestaconcept/PrestaSonataGedmoDoctrineExtensions",
    "license": "MIT",
    "authors": [
        {
            "name": "Nicolas Bastien",
            "email": "nbastien@prestaconcept.net",
            "homepage": "http://www.prestaconcept.net/"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.2",
        "sonata-project/admin-bundle":"2.2.*",
        "presta/sonata-navigation-bundle": "1.0.*"
    },
    "suggest": {
        "presta/cms-core-bundle": "1.0.*"
    },
    "autoload": {
        "psr-0": { "Presta\\SonataGedmoDoctrineExtensions": "" }
    },
    "target-dir": "Presta/SonataGedmoDoctrineExtensions",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
